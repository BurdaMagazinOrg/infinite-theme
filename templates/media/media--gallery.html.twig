{#
/**
 * @file
 * Default theme implementation to present a media entity.
 *
 * Available variables:
 * - name: Name of the media.
 * - content: Media content.
 *
 * @see template_preprocess_media()
 *
 * @ingroup themeable
 */
#}
{% if content %}
<div class="item-media-gallery gallery-container" data-view-type="gallerySlickView" data-media-id="{{ media_id }}">
    {{ content }}
</div>
{% endif %}
